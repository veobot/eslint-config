{
  "name": "@veobot/eslint-config",
  "version": "1.3.2",
  "description": "Veo Robotics ESLint rules for all JavaScript / TypeScript projects",
  "homepage": "https://github.com/veobot/eslint-config",
  "bugs": "https://github.com/veobot/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/veobot/eslint-config"
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "typescript",
    "vue",
    "es2019",
    "nodejs",
    "lint"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "postclean": "yarn install",
    "lint": "yarn lint:js && yarn lint:md && yarn lint:unused",
    "lint:js": "eslint index.js rules/**/*.js test/**/pass.* test/*.js",
    "lint:unused": "eslint --report-unused-disable-directives index.js rules/**/*",
    "lint:md": "markdownlint README.md CHANGELOG.md",
    "pretest": "yarn lint",
    "test": "jest",
    "find-rules": "eslint-find-rules --unused",
    "prepublishOnly": "yarn test"
  },
  "main": "index.js",
  "peerDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@vue/eslint-config-typescript": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^6.0.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "typescript": "^3.7.2",
    "vue-eslint-parser": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "@vue/eslint-config-typescript": "5.0.1",
    "eslint": "6.7.2",
    "eslint-find-rules": "3.4.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-vue": "6.0.1",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "jest": "24.9.0",
    "markdownlint-cli": "0.19.0",
    "shelljs": "0.8.3",
    "typescript": "3.7.3",
    "typescript-eslint-parser": "^22.0.0",
    "vue": "2.6.10",
    "vue-eslint-parser": "7.0.0",
    "vue-i18n": "^8.15.1"
  }
}
